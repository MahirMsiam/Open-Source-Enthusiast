import java.util.*;
public class Main
{
    public static void main (String[] args)
    {
   Scanner sc=new Scanner(System.in);
   int t=sc.nextInt();
   int a,b,c;
   int i;
   for(i=1;i<=t;i++)
   {
        a=sc.nextInt();
        b=sc.nextInt();
        c=sc.nextInt();
        if(a==b&&b==c)
        System.out.println("0");
        else
        System.out.println("1");
      }
        }
   }
